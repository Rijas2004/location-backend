{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(._)",
      "dest": "app.js",
      "headers": {
        "Access-Control-Allow-Origin": "_",
        "Access-Control-Allow-Methods": "GET, POST, PATCH, DELETE",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "limits": {
    "bodySize": 50
  }
}
